//Code for Hello World in C-Programming
#include <stdio.h>  // Include the standard input-output library

// Main function: the starting point of the program
int main() {
    // Print the string "Hello, World!" followed by a newline character
    printf("Hello, World!\n");

// Print the string "Welcome to Software Development for Embedded System lecture" followed by a newline character
    printf("Welcome to Software Development for Embedded System lecture\n");

    // Return 0 to indicate that the program ended successfully
    return 0;
}
